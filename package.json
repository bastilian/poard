{
  "name": "poard",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:remix": "remix build",
    "dev": "run-p dev:*",
    "dev:remix": "NODE_ENV=development npx remix dev",
    "dev:studio": "BROWSER=none DATABASE_URL=\"file:./data.db?connection_limit=1\" prisma studio",
    "format": "eslint --fix --cache --cache-location ./node_modules/.cache/eslint .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "setup": "DATABASE_URL=\"file:./data.db?connection_limit=1\" prisma migrate dev",
    "start": "remix-serve build",
    "typecheck": "tsc -b",
    "validate": "run-p \"test -- --run\" lint typecheck ",
    "github:scrape": "ts-node -r tsconfig-paths/register ./scripts/scrape.ts",
    "todos": "ts-node -r tsconfig-paths/register ./scripts/todos.ts"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@octokit/rest": "^18.12.0",
    "@patternfly/patternfly": "^4.202.1",
    "@patternfly/react-core": "^4.224.1",
    "@prisma/client": "^4.1.1",
    "@remix-run/node": "^1.6.0",
    "@remix-run/react": "^1.6.7",
    "@remix-run/serve": "^1.6.0",
    "@remix-run/server-runtime": "^1.6.0",
    "moment": "^2.29.4",
    "react": "^17.0.2",
    "react-avatar": "^5.0.1",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.6.7",
    "@remix-run/eslint-config": "^1.6.7",
    "@testing-library/react": "^12.1.5",
    "@types/eslint": "^8.4.5",
    "@types/node": "^17.0.45",
    "@types/react": "^17.0.48",
    "@types/react-dom": "^17.0.17",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "@vitejs/plugin-react": "^2.0.0",
    "c8": "^7.12.0",
    "eslint": "^8.21.0",
    "jsdom": "^20.0.0",
    "npm-run-all": "^4.1.5",
    "prisma": "^4.1.1",
    "start-server-and-test": "^1.14.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.4",
    "vitest": "^0.20.3"
  },
  "engines": {
    "node": ">=16"
  }
}
