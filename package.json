{
  "name": "poard",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:remix": "remix build",
    "dev": "run-p dev:*",
    "dev:remix": "NODE_ENV=development npx remix watch",
    "dev:server": "NODE_ENV=development node ./server.js",
    "dev:studio": "BROWSER=none DATABASE_URL=\"file:./data.db?connection_limit=1\" prisma studio",
    "format": "eslint --fix --cache --cache-location ./node_modules/.cache/eslint .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "setup": "DATABASE_URL=\"file:./data.db?connection_limit=1\" prisma migrate dev",
    "start": "remix-serve build",
    "typecheck": "tsc -b",
    "validate": "run-p \"test -- --run\" lint typecheck ",
    "github:scrape": "ts-node ./scripts/scrape.ts",
    "todos": "ts-node ./scripts/todos.ts"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@octokit/rest": "^19.0.7",
    "@patternfly/patternfly": "^4.224.2",
    "@patternfly/react-core": "^4.224.1",
    "@patternfly/react-icons": "^4.93.6",
    "@prisma/client": "^4.14.0",
    "@remix-run/node": "^1.8.2",
    "@remix-run/react": "^1.16.0",
    "@remix-run/serve": "^1.16.0",
    "@remix-run/server-runtime": "^1.8.2",
    "chalk": "^4.1.2",
    "express": "^4.18.2",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "react": "^17.0.2",
    "react-avatar": "^5.0.3",
    "react-dom": "^17.0.2",
    "remix-auth": "^3.4.0",
    "remix-auth-github": "^1.3.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.16.0",
    "@remix-run/eslint-config": "^1.16.0",
    "@testing-library/react": "^12.1.5",
    "@types/eslint": "^8.37.0",
    "@types/node": "^18.16.7",
    "@types/react": "^17.0.59",
    "@types/react-dom": "^17.0.20",
    "@types/yargs": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "@vitejs/plugin-react": "^3.1.0",
    "c8": "^7.13.0",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jsdom": "^20.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "prisma": "^4.14.0",
    "start-server-and-test": "^1.15.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.5",
    "vite": "^4.3.5",
    "vitest": "^0.25.8"
  },
  "engines": {
    "node": ">=18"
  }
}
